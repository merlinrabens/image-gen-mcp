# Image Generation Provider API Keys
# Copy this file to .env and fill in your API keys

# OpenAI API Key for DALL-E image generation
OPENAI_API_KEY=

# Stability AI API Key for Stable Diffusion
STABILITY_API_KEY=

# Replicate API Token for various models
REPLICATE_API_TOKEN=

# Google Gemini API Key for Imagen
GEMINI_API_KEY=

# Default provider when none specified
# Options: OPENAI, STABILITY, REPLICATE, GEMINI, MOCK
DEFAULT_PROVIDER=GEMINI

# Logging level
# Options: info, debug, error
LOG_LEVEL=info